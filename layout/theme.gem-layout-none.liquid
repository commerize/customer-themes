{% comment %}
	GEMPAGE BUILDER (https://apps.shopify.com/gempage)

	You SHOULD NOT modify source code in this page because
	It is automatically generated from GEMPAGE BUILDER
	Try to edit page with the live editor.
{% endcomment %}
<!DOCTYPE html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Establish early connection to external domains -->
  <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  <link rel="preconnect" href="https://fonts.shopify.com" crossorigin>
  <link rel="preconnect" href="https://monorail-edge.shopifysvc.com">
  <link rel="preconnect" href="//ajax.googleapis.com" crossorigin />

  {%- liquid

    assign main_font = settings.main_font
    assign heading_font = settings.heading_font
    assign nav_font = settings.nav_font
    assign main_font_bold = main_font | font_modify: 'weight', 'bolder'
    assign main_font_italic = main_font | font_modify: 'style', 'italic'
    assign main_font_bold_italic = main_font_bold | font_modify: 'style', 'italic'

  -%}

  <!-- Preload onDomain stylesheets and script libraries -->
  <link rel="preload" href="{{ 'stylesheet.css' | asset_url }}" as="style">
  <link rel="preload" as="font" href="{{ main_font | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ nav_font | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" as="font" href="{{ heading_font | font_url }}" type="font/woff2" crossorigin>
  <link rel="preload" href="{{ 'eventemitter3.min.js' | asset_url }}" as="script">
  <link rel="preload" href="{{ 'vendor-video.js' | asset_url }}" as="script">
  <link rel="preload" href="{{ 'theme.js' | asset_url }}" as="script">


  {% if settings.favicon %}<link rel="shortcut icon" href="{{ settings.favicon | image_url: width: 32, height: 32 }}" type="image/png" />{% endif %}
  <link rel="canonical" href="{{ canonical_url }}" />

  <title>{% if template == 'index' %}{% if page_title contains 'Welcome' %}{{ shop.name }} &ndash; Welcome{% else %}{{ page_title }}{% endif %}{% elsif template == '404' %}Page Not Found{% elsif page_title contains shop.name %}{{ page_title }}{% else %}{{ page_title }} &ndash; {{ shop.name }}{% endif %}</title>
  {% if page_description %}<meta name="description" content="{{ page_description }}" />{% endif %}

  {% render 'social-media-meta-tags' %}
  {% render 'structured-data' %}
  {% render 'css-variables' %}

  <link rel="stylesheet" href="{{ 'stylesheet.css' | asset_url }}" type="text/css">

  <style>


		/* ibm-plex-sans-300 - latin */
		@font-face {
			font-family: 'IBM Plex Sans';
			font-style: normal;
			font-weight: 300;
			src: url('{{ "ibm-plex-sans-v2-latin-300.eot" | asset_url }}'); /* IE9 Compat Modes */
			src: local('IBM Plex Sans Light'), local('IBMPlexSans-Light'),
				url('{{ "ibm-plex-sans-v2-latin-300.eot" | asset_url }}?#iefix') format('embedded-opentype'), /* IE6-IE8 */
				url('{{ "ibm-plex-sans-v2-latin-300.woff2" | asset_url }}') format('woff2'), /* Super Modern Browsers */
				url('{{ "ibm-plex-sans-v2-latin-300.woff" | asset_url }}') format('woff'), /* Modern Browsers */
				url('{{ "ibm-plex-sans-v2-latin-300.ttf" | asset_url }}') format('truetype'), /* Safari, Android, iOS */
				url('{{ "ibm-plex-sans-v2-latin-300.svg" | asset_url }}#IBMPlexSans') format('svg'); /* Legacy iOS */
		}
		/* karla-700 - latin */
		@font-face {
			font-family: 'Karla';
			font-style: normal;
			font-weight: 700;
			src: url('{{ "karla-v6-latin-700.eot" | asset_url }}'); /* IE9 Compat Modes */
			src: local('Karla Bold'), local('Karla-Bold'),
				url('{{ "karla-v6-latin-700.eot" | asset_url }}?#iefix') format('embedded-opentype'), /* IE6-IE8 */
				url('{{ "karla-v6-latin-700.woff2" | asset_url }}') format('woff2'), /* Super Modern Browsers */
				url('{{ "karla-v6-latin-700.woff" | asset_url }}') format('woff'), /* Modern Browsers */
				url('{{ "karla-v6-latin-700.ttf" | asset_url }}') format('truetype'), /* Safari, Android, iOS */
				url('{{ "karla-v6-latin-700.svg" | asset_url }}#Karla') format('svg'); /* Legacy iOS */
		}
		/* reem-kufi-regular - latin */
		@font-face {
			font-family: 'Reem Kufi';
			font-style: normal;
			font-weight: 400;
			src: url('{{ "reem-kufi-v4-latin-regular.eot" | asset_url }}'); /* IE9 Compat Modes */
			src: local('Reem Kufi Regular'), local('ReemKufi-Regular'),
				url('{{ "reem-kufi-v4-latin-regular.eot" | asset_url }}?#iefix') format('embedded-opentype'), /* IE6-IE8 */
				url('{{ "reem-kufi-v4-latin-regular.woff2" | asset_url }}') format('woff2'), /* Super Modern Browsers */
				url('{{ "reem-kufi-v4-latin-regular.woff" | asset_url }}') format('woff'), /* Modern Browsers */
				url('{{ "reem-kufi-v4-latin-regular.ttf" | asset_url }}') format('truetype'), /* Safari, Android, iOS */
				url('{{ "reem-kufi-v4-latin-regular.svg" | asset_url }}#ReemKufi') format('svg'); /* Legacy iOS */
		}
    {% if collection %}
     .grid__wrapper.full {
       margin-bottom:-20px;
     }
    {% endif %}
	</style>

  {{ content_for_header }}

<!--Gem_Page_Header_Script-->
{% render 'gem-app-header-scripts' %}
<!--End_Gem_Page_Header_Script-->
</head>
<body>
<div class="gem-container">
	{{ content_for_layout }}
</div><!-- end .gem-container -->
<!--Gem_Page_Footer_Script-->
  {%- liquid
    assign gpTemplateSuffix = template.suffix
    assign arrSplitSuffix = gpTemplateSuffix | split: '-'
    if gpTemplateSuffix contains 'gem-' and gpTemplateSuffix contains '-template' and arrSplitSuffix.size == 3
      assign isV6GpTemplate = true
    endif
    assign gpShopMeta = shop.metafields.gempages
    if gpShopMeta
      if gpShopMeta['productV6-default'] == 'true' and request.page_type == 'product'
        assign isProductDefault = true
      endif
      if gpShopMeta['collectionV6-default'] == 'true' and request.page_type == 'collection'
        assign isCollectionDefault = true
      endif
      if gpShopMeta['indexV6-default'] == 'true' and request.page_type == 'index'
        assign isIndexDefault = true
      endif
      if isProductDefault or isCollectionDefault or isIndexDefault or isPreviewV6
        assign isV6GpTemplate = true
      endif
    endif
  %}
  {% if isV6GpTemplate %}
    {% render 'gem-app-footer-scripts', GEM_FOOTER_SCRIPT: GEM_FOOTER_SCRIPT %}
  {% endif %}
<!--End_Gem_Page_Footer_Script-->




</body>
</html>
